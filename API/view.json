{
    "Method" : "GET",
    "Query": "api.pmccabe_collector.restapi.org/project/{uuid}/statistic/view",
    "Params": {
        "statistic" : {
            "watch_list" : {
                "-regex": "\".*\\.\\(hpp\\|cpp\\|c\\|h\\)\"",
                "-prune": "! -path \"*buil*\" ! -path \"*thirdpart*\""
            },
            "-mmcc_range" : "1,999999",
            "-tmcc_range" : "1,999999",
            "-sif_range" : "1,999999",
            "-lif_range" : "1,999999"
        },
        "attributes" : "mmcc,tmcc,sif,lif"
    }
}
